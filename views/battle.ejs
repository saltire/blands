<!doctype html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Blattle of the Blands</title>
    <link rel='stylesheet' href='/style.css'>
  </head>
  <body>
    <% rounds.forEach((round, i) => { %>
      <h2>Round <%= i + 1 %></h2>
      <% round.performances.forEach((perf) => { %>
        <p><span style='color: <%= perf.band.color %>'><%= perf.band.name %></span> - <%= perf.song.name %> - <b><%= perf.score %></b></p>
      <% }); %>
      <h3>Eliminated: <span style='color: <%= round.eliminee.color %>'><%= round.eliminee.name %></span></h3>
    <% }); %>

    <h2>Winner: <span style='color: <%= rankedBands[0].color %>'><%= rankedBands[0].name %></span></h2>
  </body>
</html>
