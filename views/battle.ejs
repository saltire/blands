<!doctype html>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Blattle of the Blands</title>
    <link rel='stylesheet' href='/style.css'>
  </head>
  <body>
    <% rounds.forEach((round, i) => { %>
    <h2>Round <%= i + 1 %></h2>
    <p>
      <% round.performances.forEach((perf) => { %>
        <span style='color: <%= perf.band.color %>'><%= perf.band.name %></span> - <%= perf.song %> - <b><%= perf.score %></b><br>
      <% }) %>
    </p>
    <p>Eliminated: <span style='color: <%= round.eliminee.color %>'><%= round.eliminee.name %></span></p>
    <% }) %>

    <h2>Winner: <span style='color: <%= winner.color %>'><%= winner.name %></span></h2>
  </body>
</html>
