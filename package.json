{
  "name": "blands",
  "private": true,
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start": "node built/server.js",
    "watch": "nodemon -e ts,json,sql -x \"ts-node -r dotenv/config src/server.ts\"",
    "build": "tsc",
    "lint": "eslint ./src",
    "schema": "ts-node -r dotenv/config src/schema.ts",
    "migrations": "pg-migrations apply --directory ./sql/migrations"
  },
  "dependencies": {
    "@databases/pg": "^5.4.1",
    "@databases/pg-migrations": "^5.0.1",
    "@databases/pg-schema-cli": "^4.3.0",
    "@databases/pg-typed": "^4.3.0",
    "@ff00ff/mammoth": "^1.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.1.3"
  }
}
